<%- include ('partials/top') %>
<div class="row">
  <div class="col-12 py-4">
    <div class="d-flex justify-content-between">
      <h2>Deposits Transactions</h2>
      <div>
        <a href="/accounts/transactions/withdrawals" class="btn btn-outline-primary">View Withdrawals</a>
      </div>
    </div>
  </div>

  <div class="col-12 py-4">
    <h4 class="mb-0 bg-info py-2 text-white text-center mt-4">List of Withdrawals</h4>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Sender Account No.</th>
          <th>Sender Name</th>
          <th>Receiver Account No.</th>
          <th>Receiver Name</th>
          <th>Amount</th>
          <th>Date of Transaction</th>
        </tr>
      </thead>
      <tbody>
        <% transactions.forEach(m => { %>
          <tr>
            <td><%= m.source.account_number %></td>
            <td><%= m.source.account_holder_name %></td>
            <td><%= m.receiver.account_number %></td>
            <td><%= m.receiver.account_holder_name %></td>
            <td><%= m.amount %></td>
            <td><%= m.createdAt.toGMTString() %></td>
          </tr>
        <% }) %>
        <tr>
      </tbody>
    </table>
  </div>
</div>
</div>

<%- include ('partials/bottom') %>