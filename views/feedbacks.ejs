<%- include ('partials/top') %>
<div class="row">
  <div class="col-12">
    <h4>Feedbacks</h4>
  </div>

  <div class="col-lg-6 offset-lg-3 col-md-8 offset-md-2">

    <table class="table table-striped table-dark">
      <thead>
        <th>From</th>
        <th>Account No.</th>
        <th>Contact</th>
        <th>Message</th>
      </thead>

      <tbody>
        <% feedbacks.forEach( f => { %>
          <tr>
            <td><%= f.sender_name  %></td>
            <td><%= f.from_account_number  %></td>
            <td><%= f.contact_phone  %></td>
            <td><%= f.message  %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>

  </div>

  <div>
    <h4><%= bank_name %></h4>
  </div>
</div>
<%- include ('partials/bottom') %>